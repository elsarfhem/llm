def solution():
    
    num_gifts_family = 6
    num_gifts_friends = 4
    num_gifts_teachers = 2
    total_gifts_to_wrap = num_gifts_family + num_gifts_friends + num_gifts_teachers
    ribbon_total_inches = 144
    bow_per_gift = ribbon_total_inches / total_gifts_to_wrap
    result = bow_per_gift
    return result







