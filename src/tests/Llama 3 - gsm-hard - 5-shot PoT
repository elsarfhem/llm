    jello_cups_per_kid  = 4
    total_jello_cups  = kids * jello_cups_per_kid
    three_ounce_box_jello_cups  = 10
    boxes_needed  = total_jello_cups / three_ounce_box_jello_cups
    cost_per_box  = 1.25
    result  = boxes_needed * cost_per_box
    return result






