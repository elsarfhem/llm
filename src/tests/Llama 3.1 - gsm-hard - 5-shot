    distance = 60
    cost_per_mile = 2
    number_of_weeks = 50
    number_of_days = 5
    bonus_per_month = 150
    months_in_year = 12
    total_distance = distance * number_of_days * number_of_weeks * 2
    total_cost = total_distance * cost_per_mile + (bonus_per_month * months_in_year)
    result = total_cost
    return result






