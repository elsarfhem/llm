    morning_cost = 6
    afternoon_cost = 2
    days = 14
    morning_rides_per_week = days * rides_per_day
    total_morning_rides = morning_rides_per_week
    total_afternoon_rides = morning_rides_per_week
    total_rides = total_morning_rides + total_afternoon_rides
    total_money_spent = total_morning_rides * morning_cost + total_afternoon_rides * afternoon_cost
    result = total_money_spent
    return result







