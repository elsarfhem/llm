answer:    

def solution():
    
    num_rides = 9
    ticket_price_per_ticket = 3078953
    ticket_price_for_bracelet = 30
    ticket_price_per_ride_bracelet = ticket_price_for_bracelet / num_rides
    ticket_cost_dasha = (num_rides * ticket_price_per_ticket)
    ticket_cost_david = ticket_price_for_bracelet
