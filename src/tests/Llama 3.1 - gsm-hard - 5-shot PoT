def solution():
    
    choc_icecream_per_hour = 100 / 2
    van_icecream_per_hour = 50 / 4
    combined_quarts_per_hour = (choc_icecream_per_hour + van_icecream_per_hour)
    total_hours = 48
    result = combined_quarts_per_hour * total_hours
    return result







