    new_cake_slices = 8647390
    total_cake_slices = cake_slices_initial + new_cake_slices
    tripled_cake_slices = total_cake_slices * 3
    eaten_slices = 2
    stolen_slices = 5
    remaining_slices = tripled_cake_slices - eaten_slices - stolen_slices
    result = remaining_slices
    return result






