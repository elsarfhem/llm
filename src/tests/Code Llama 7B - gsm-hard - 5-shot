    num_voted_at_8am = 2/5 * initial_num_people
    num_remaining_at_noon = initial_num_people - num_voted_at_8am
    num_voted_at_4pm = 2/3 * num_remaining_at_noon
    num_remaining_at_4pm = initial_num_people - num_voted_at_4pm
    unvoted_by_4pm = num_remaining_at_4pm
    result = unvoted_by_4pm
    return result
