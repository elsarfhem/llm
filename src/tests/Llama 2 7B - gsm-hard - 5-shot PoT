answer:   

def solution():
    
    meat_cost = 7.0
    cheese_cost = 3.0
    sandwiches = 4
    total_people = 5979089
    total_meat_needed = total_people * sandwiches * meat_cost
    total_cheese_needed = total_people * sandwiches * cheese_cost
    result = total_meat_needed + total_cheese_needed
    return result






