answer:   

def solution():
    
    miles_per_week = 30
    miles_per_day = miles_per_week / 5
    days_per_week = 5
    weeks_per_month = 4
    days_per_month = days_per_week * weeks_per_month
    total_miles_driven = miles_per_day * days_per_month
    bonus = 150
    cost_per_mile = 2
    cost = total_miles_driven * cost_per_mile + bonus
    result = cost
    return result







