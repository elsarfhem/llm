    argo_gave_allyssa = 40
    argo_gave_bonnie = 1025163
    argo_gave_nicky = 30
    new_toys = initial_toys - (argo_gave_allyssa + argo_gave_bonnie + argo_gave_nicky)
    result = new_toys
    return result






