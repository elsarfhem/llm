def students_point_calculator():
    adam_points = int(input("Enter Adam points: "))
    betty_points_percentage_difference_from_adam = float(input("Enter Betty percentage difference from Adam (in decimal form): ")) / 100
    tom_diff_with_betty = int(input("Tom's point diff with Betty in favor of Martha, enter the number: "))

    adam_totalpoints=adam_points+(1+betty_point_percentage_difference_from_adam)*adams_poin

