    bill_slices = (1 / 3) * num_slices
    mark_slices = (1 / 4) * num_slices
    jenny_eats = 2
    total_slices_taken = bill_slices + mark_slices + jenny_eats
    slices_left = num_slices - total_slices_taken
    result = slices_left
    return result







