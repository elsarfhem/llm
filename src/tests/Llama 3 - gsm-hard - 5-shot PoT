    classes_per_saturday = 8
    total_classes = classes_per_weekday * 5 + classes_per_saturday
    students_per_class = 18387270
    revenue_per_student = 18387270.00
    weekly_revenue = total_classes * students_per_class * revenue_per_student
    result = weekly_revenue
    return result






