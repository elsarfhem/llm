    stripes_zebra2 = 3009606
    stripes_zebra3 = 3009606 / 2
    total_stripes = stripes_zebra1 + stripes_zebra2 + stripes_zebra3
    average_stripes = total_stripes / (2 + 1)
    result = average_stripes
    return result






