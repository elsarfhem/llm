    time_reading = hours_watching_tv / 2
    number_of_weeks = 4
    total_time_spent_on_tv = hours_watching_tv * number_of_weeks
    total_time_spent_reading = time_reading * number_of_weeks * 3
    total_time_spent = total_time_spent_on_tv + total_time_spent_reading
    result = total_time_spent
    return result







