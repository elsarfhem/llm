answer:   

def solution():
    
    driving_speed = 50
    driving_time = 3600 / driving_speed
    walking_speed = (driving_speed + 25) / 2
    walking_time = 1800 / walking_speed
    total_distance = driving_time + walking_time
    result = total_distance * walking_speed
    return result







