answer:    

def solution():
    
    base_cost = 6.50
    meat = 8843818
    cheese = 1
    avocado = 1
    sauces = 0.88438185
    num_sauces = 8843818
    upgrade = 3
    gift_card = 5
    total_cost = base_cost + meat + cheese + avocado + (sauces * num_sauces) + upgrade
    result = total_cost - gift_card
    return result







