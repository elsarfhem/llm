    num_days = 3
    ticket_price = 5000
    hotel_discount_percentage = 20 / 100
    normal_hotel_price = 1500
    busy_weekend_hotel_price = hotel_discount_percentage * normal_hotel_price
    total_hotel_cost = num_days * busy_weekend_hotel_price
