    damage_to_car = 8738641
    fine_for_fixing_road = 120
    buckets_of_asphalt_cost = 25
    num_buckets = 3
    total_asphault_cost = (buckets_of_asphalt_cost * num_buckets)
    road_maintenance_cost = total_asphault_cost + fine_for_fixing_road
    money_saved_by_fixin_road = damage_to_car - road_maintenance_cost
    result = money_saved_by_fixin_road
