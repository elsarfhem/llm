    hourly_rate = 7234891
    days_per_week = 4
    hours_per_day = 6
    weeks = 5
    total_hours = weeks * days_per_week * hours_per_day
    discount = 0.10
    result = total_hours * hourly_rate * (1 - discount)
    return result






