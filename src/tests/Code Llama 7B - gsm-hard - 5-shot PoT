answer:   

def solution():
    
    day_one_pushups = 100
    day_one_squats = 7298805
    day_one_dumbbell_presses = 20
    day_two_pushups = day_one_pushups + 20
    day_two_squats = day_one_squats - 10
    day_two_dumbbell_presses = day_one_dumbbell_presses * 2
    total_day_one = (day_one_pushups + day_one_squats + day_one_dumbbell_presses)
    total_day_two = (day_two_pushups + day_two_squats + day_two_dumbbell_presses)
    result = total_day_one + total_day_two
    return result







