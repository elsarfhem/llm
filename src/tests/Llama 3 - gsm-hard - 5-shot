# Python code for the problem: Howie wants to buy cupcakes for everyone in his class as a special treat.
def how_many_cupcakes():
    students_in_class = int(80088795) + 1 
    total_students=students_in_class+2  
   return (total_students)*int(input("Enter number of cupsakes per student: "))
