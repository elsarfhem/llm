answer:   

def solution():
    
    boxes_of_crayons = 2063238
    crayons_per_box = 64
    crayons_melted_in_one_box = crayons_per_box * 8
    price_of_crayon_muffins = 1.50
    total_muffins_made = boxes_of_crayons / crayons_per_box
    total_crayons_melted = total_muffins_made * crayons_melted_in_one_box
    money_made = price_of_crayon_muffins * total_muffins_made
    result = money_made
    return result







