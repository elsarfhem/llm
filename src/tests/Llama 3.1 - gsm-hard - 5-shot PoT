answer:    

def solution():
    
    weekday_chairs_rented = 60
    weekend_chairs_rented = 4141490
    weekdays_in_2_months = 2 * 4 * 5
    weekends_in_2_months = 2 * 4 * 2
    total_weekday_chairs_rented = weekday_chairs_rented * weekdays_in_2_months
    total_weekend_chairs_rented = weekend_chairs_rented * weekends_in_2_months
