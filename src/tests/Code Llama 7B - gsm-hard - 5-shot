    total_scarves = num_grandkids * scarves_per_grandchild
    total_sweaters = num_grandkids * sweaters_per_grandchild
    total_mittens = num_grandkids / 2
    total_socks = num_grandkids / 2
    result = total_hats + total_scarves + total_sweaters + total_mittens + total_socks
    return result






