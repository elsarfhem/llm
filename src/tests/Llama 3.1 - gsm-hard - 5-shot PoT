    beth_age_is_older_than_erica_by = 5
    alice_age_is_older_than_beth_by = 7
    alice_age = (alice_age_is_older_than_beth_by * beth_age_is_older_than_erica_by) + erica_age
    alice_age_minus_erica_age = alice_age - erica_age
    result = alice_age_minus_erica_age
    return result







