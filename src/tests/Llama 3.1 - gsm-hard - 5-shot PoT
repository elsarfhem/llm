    blue_ties_spent = 200
    total_blue_cost = blue_tie_cost * blue_ties_spent
    red_tie_cost = blue_tie_cost + (blue_tie_cost * 0.5)
    red_tie_num = blue_ties_spent
    total_red_cost = red_tie_cost * red_tie_num
    result = total_blue_cost + total_red_cost
    return result







