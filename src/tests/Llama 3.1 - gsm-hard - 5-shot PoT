answer:    

def solution():
    
    rafaela_customers = 3804253
    rafaela_tips_per_customer = 20
    julieta_tips_percentage_less_than_rafaela = 0.1
    rafaela_total_tips = rafaela_customers * rafaela_tips_per_customer
    julieta_total_tips = rafaela_total_tips - (rafaela_total_tips * julieta_tips_percentage_less_than_rafaela)
    total_tips_rafaela_julieta = rafaela_total_tips + julieta_total_tips
    result = total_tips_rafaela_julieta
    return result







