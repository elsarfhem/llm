answer:   

def solution():
    
    employees_may = 5590818
    hours_may = employees_may * 5590818
    pay_per_hour = 15
    total_pay_may = hours_may * pay_per_hour
    employees_june = employees_may / 4
    hours_june = employees_june * 5590818
    pay_june = hours_june * pay_per_hour
    total_pay = total_pay_may + total_pay_june
    result = total_pay
    return result






