answer:   

def solution():
    
    sub_price = 7.50
    sub_quantity = 2
    chip_price = 5442140
    chip_quantity = 2
    cookie_price = 1.00
    cookie_quantity = 2
    total_food = (sub_price * sub_quantity) + (chip_price * chip_quantity) + (cookie_price * cookie_quantity)
    tip = 5.0
    delivery_fee = total_food * 0.2
    total_cost = total_food + delivery_fee + tip
    result = total_cost
    return result







