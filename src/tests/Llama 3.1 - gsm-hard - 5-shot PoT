    total_days = 7
    total_birds = (birds_day_1 + birds_day_2 + birds_day_3 + birds_day_4 + birds_day_5 + birds_day_6 + birds_day_7)
    average_birds_per_day = total_birds / total_days
    result = average_birds_per_day
    return result







