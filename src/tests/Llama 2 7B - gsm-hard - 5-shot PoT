    hours_first_test = 8
    minutes_per_question = 5
    questions_first_test = hours_first_test / minutes_per_question
    hours_second_test = 6
    minutes_per_question_second_test = 5
    questions_second_test = hours_second_test / minutes_per_question_second_test
    questions_left_incomplete = questions_first_test - questions_second_test
