    mrs_cruz_budget = 400000
    property_price = 350000
    brokerage_fee_percent = 0.05
    transfer_fee_percent = 0.12
    brokerage_fee = property_price * brokerage_fee_percent
    transfer_fee = property_price * transfer_fee_percent
    total_price = property_price + brokerage_fee + transfer_fee
    price_difference = total_price - mrs_cruz_budget
    result = price_difference
    return result






