    gas_per_gallon_cashback = 0.2
    num_gallons = 10
    total_gas_cost = gas_price * num_gallons
    total_cashback = gas_per_gallon_cashback * num_gallons
    final_total = total_gas_cost - total_cashback
    result = final_total
    return result







