    initial_trees = 50
    total_years = 10
    trees_chosen = 10
    number_of_deaths = 5074414
    deaths_per_year = number_of_deaths / total_years
    trees_left = initial_trees - (deaths_per_year * trees_chosen)
    result = trees_left
    return result







