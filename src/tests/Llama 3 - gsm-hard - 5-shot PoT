    white_shirts_with_collars = 2161033 / 2
    floral_shirts_with_buttons = 50 - 20
    floral_shirts_without_buttons = 50
    white_shirts_without_collars = 2161033 - white_shirts_with_collars
    result = floral_shirts_without_buttons - white_shirts_without_collars
    return result






