total_penguins = int(296509)
penguin_jump_in_ocean = total_penguins // 3
remaining_sunbathe_after_jumping_into_the_ocean = total_penguins - penguin_jump_in_ocean

penguin_go_inside_cave_to_eat_dinner, remaining_sunbathing_after_swimming_and_got_hungry_against_food_at_home \
= divmod(penguin_jump_in_ocean // 3)

