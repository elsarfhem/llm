    eggs_cost = 3
    light_bulbs_cost = 3
    cups_cost = 3
    roach_traps_cost = 4
    
    # calculate the total price of non-food items
    non_food_items_price = (light_bulbs_cost + cups_cost + roach_traps_cost) * 1.10
    
    # calculate the total price of food items
    food_items_price = milk_cost + eggs_cost

    # calculate the total price
    total_price = non_food_items_price + food_items_price

    result = total_price
    return result






