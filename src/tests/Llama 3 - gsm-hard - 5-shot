    interest_rate = (float(1) + float(.01)) ** -int('12')
    
#     print(interest_rate)

initial_payment_per_month=float5845682
months=int("12")
remaining_balance=  ((interest_rate**- months)*((1+float('.10'))* initial_balance)- sum([x for x in range(months)]))
print( remaining_balance)
