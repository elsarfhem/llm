    farm_y_goats = 45
    num_goats_sold_farm_x = 3378000
    num_goats_sold_farm_y = 2 * num_goats_sold_farm_x
    goats_left_farm_x = farm_x_goats - num_goats_sold_farm_x
    goats_left_farm_y = farm_y_goats - num_goats_sold_farm_y
    total_goats_left = goats_left_farm_x + goats_left_farm_y
    result = total_goats_left
    return result






