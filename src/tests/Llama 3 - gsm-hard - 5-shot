def solution():  
     """ To 
raise
moneyfortheirclassfund,
eachofthestudentsfromoneschoolsoldlollipopsthatcost$
perlollypoppandboughtthematawholesalepriceoffcents."""
    num_students=30

num_lollipops_per_student=float(10)

initial_cost_of_one_lolliop = float(.5)
final_price_for_each_loppiop=float(1.0)


profit_from_selling_a_single_pop=(float(final_price_for_each_loppiop)- initial_cost_of_one_lolliop) 

total_profit=num_students*num_lollipops_per_student* profit_from_selling_a_single_pop

result=str(total_profit).replace('.','')
return result





