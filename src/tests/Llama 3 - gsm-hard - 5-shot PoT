    basketball_cards_per_deck  = 25
    basketball_cards_total  = basketball_decks * basketball_cards_per_deck
    baseball_boxes  = 9668743
    baseball_cards_per_box  = 40
    baseball_cards_total  = baseball_boxes * baseball_cards_per_box
    total_cards  = basketball_cards_total + baseball_cards_total - 50
    students_10_cards  = total_cards / 10
    result  = int(students_10_cards) # convert to integer
    return result






