answer:    

def solution():
    
    rottweiler_pounds = 8
    chihuahua_pounds = 2
    num_collies = 3
    num_chihus = 4
    total_pounds = (chihuahua_pounds * num_chihus) + ((num_collies * (rottweiler_pounds + chihuahua_pounds)) / 2)
