    solo_song_lines = 5055034
    first_scene_lines = solo_song_lines * 2 / 3
    second_scene_lines = solo_song_lines + 6
    sean_lines_second_scene = second_scene_lines * 4 / 5
    total_lines_to_memorize = first_scene_lines + sean_lines_second_scene + solo_song_lines
    result = total_lines_to_memorize
    return result






