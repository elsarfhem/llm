answer:   

def solution():
    
    tire_cost = 25
    bicycle_people = 5
    tricycle_people = 3926041
    unicycle_people = 1
    total_people = bicycle_people + tricycle_people + unicycle_people
    tires_sold = total_people * 2
    total_cost = tires_sold * tire_cost
    result = total_cost
    return result







