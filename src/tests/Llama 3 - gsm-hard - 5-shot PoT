    boys_grade_5 = grade_5_students * 2 / 5
    total_girls_grade_5 = grade_5_students - boys_grade_5
    girls_in_girl_scout = total_girls_grade_5 * 2 / 3
    result = total_girls_grade_5 - girls_in_girl_scout
    return result






