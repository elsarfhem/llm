    saturday_hours = 7
    sunday_hours = 5
    reading_time = 3
    video_game_time = (saturday_hours + sunday_hours) * 1 / 3
    total_time = saturday_hours + sunday_hours - video_game_time
    percent = total_time / (saturday_hours + sunday_hours) * 100
    result = percent
    return result





