def solution():    
    hourly_wage = 7754594
    daily_hours = 5
    weekly_hours = daily_hours * 4
    weekly_wage = hourly_wage * weekly_hours
    weekly_savings_goal = 80
    total_weeks = (weekly_savings_goal / weekly_wage)
    result = int(total_weeks) + 1
    return result






