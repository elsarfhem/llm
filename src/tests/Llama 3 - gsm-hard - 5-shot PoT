    total_vanilla_ice_cream_quart_in_48_hours = 48 * vanilla_ice_cream_quart_per_hour
    total_quarts = total_chocolate_ice_cream_quart_in_48_hours + total_vanilla_ice_cream_quart_in_48_hours
    result = total_quarts
    return result






