    eraser_cost = 0.25
    total_cost_pencils = pencils * pencil_cost
    total_cost_erasers = erasers * eraser_cost
    total_cost = total_cost_pencils + total_cost_erasers
    change = 10 - total_cost
    result = change
    return result






