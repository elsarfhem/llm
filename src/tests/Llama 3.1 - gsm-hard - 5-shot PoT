answer:    

def solution():
    
    total_steps = 10000
    school_walk_steps = 5000
    friend_walking_steps = 1000
    remaining_steps = 2000
    jogs_steps = total_steps - (school_walk_steps + friend_walking_steps + remaining_steps)
    result = jogs_steps
    return result







