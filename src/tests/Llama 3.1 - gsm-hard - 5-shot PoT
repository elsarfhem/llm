    people_from_4_family = 3 * 4
    num_people_total = people_from_6_family + people_from_4_family
    num_couldnt_make_it = 8
    num_previous_commitments = num_couldnt_make_it / 4
    total_show_up = num_people_total - (num_couldnt_make_it + num_previous_commitments)
