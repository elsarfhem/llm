    vehicles_imported_first_day = containers_imported_first_day * 5
    total_vehicles_after_second_day = 30
    containers_imported_second_day = (total_vehicles_after_second_day - vehicles_imported_first_day) / 5
    result = containers_imported_second_day
    return result






