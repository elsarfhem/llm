answer:    

def solution():
    
    num_people = 5979089
    servings_per_sandwich = 4
    total_servings_needed = num_people * servings_per_sandwich
    meat_cost_pound = 7.00
    cheese_cost_pound = 3.00
    meat_pounds_needed = total_servings_needed / 4
    cheese_pounds_needed = total_servings_needed / 4
    total_meat_cost = (meat_pounds_needed * meat_cost_pound) + (cheese_pounds_needed * cheese_cost_pound)
    result = total_meat_cost
    return result







