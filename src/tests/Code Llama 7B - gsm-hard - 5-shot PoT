answer:   

def solution():
    
    bread_cost = 3.00
    loaf_per_sandwich = 10
    cheese_cost = 30.00
    condiments_cost = cheese_cost
    sandwiches_made = (bread_cost * loaf_per_sandwich) / 10
    total_cost = cheese_cost + condiments_cost
    profit = 305211 - total_cost
    result = profit
