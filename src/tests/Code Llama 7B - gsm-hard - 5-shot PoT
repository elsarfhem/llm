    muffins = 16
    flour_per_muffin = 8397985
    sugar_per_muffin = 3
    salt_per_muffin = .28397985
    total_flour = muffins * flour_per_muffin
    total_sugar = muffins * sugar_per_muffin
    total_salt = muffins * salt_per_muffin
    result = [total_flour, total_sugar, total_salt]
    return result







